{
  "name": "r6-operatoricons",
  "version": "0.0.1",
  "description": "Rainbow Six: Siege operator icons made in Illustrator, available in SVG, PNG and AI",
  "author": "marcopixel <marcopixel@live.de>",
  "license": "CC-BY-4.0",
  "scripts": {
    "start": "gatsby develop",
    "develop": "gatsby develop",
    "deploy": "gatsby build --prefix-paths && gh-pages -d public",
    "build": "gatsby build",
    "type-check": "tsc --noEmit",
    "lint": "tslint --project .",
    "build:png": "node ./scripts/buildPNGs.js",
    "optimizeSVG": "svgo --folder ./icons --config ./.svgo"
  },
  "dependencies": {
    "@types/node": "^10.12.0",
    "@types/react": "16.4.18",
    "@types/react-dom": "16.0.9",
    "@types/react-helmet": "^5.0.7",
    "@types/webpack-env": "^1.13.6",
    "autoprefixer": "^9.2.0",
    "browserslist": "^4.2.1",
    "cssnano": "^4.1.4",
    "feather-icons": "^4.7.3",
    "gatsby": "2.0.25",
    "gatsby-plugin-react-helmet": "^3.0.0",
    "gatsby-plugin-resolve-src": "^2.0.0-beta.1",
    "gatsby-plugin-sass": "^2.0.1",
    "gatsby-plugin-svg-sprite": "marcobiedermann/gatsby-plugin-svg-sprite#master",
    "gatsby-plugin-typescript": "2.0.0",
    "node-sass": "^4.9.4",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-dropdown": "^1.6.2",
    "react-helmet": "^5.2.0"
  },
  "devDependencies": {
    "gh-pages": "^2.0.1",
    "svg-to-png": "^4.0.0",
    "tslint": "^5.10.0",
    "tslint-config-prettier": "^1.13.0",
    "tslint-react": "^3.6.0",
    "typescript": "^3.1.3"
  }
}
